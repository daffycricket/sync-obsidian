{
  "name": "obsidian-syncobsidian",
  "version": "1.6.0",
  "description": "Plugin de synchronisation auto-hébergé pour Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "obsidian",
    "sync",
    "self-hosted"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
